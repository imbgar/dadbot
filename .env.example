# Roboflow API Key (required)
# Get your key from: https://app.roboflow.com/settings/api
ROBOFLOW_API_KEY=your_api_key_here

# Optional: Override default calibration
# DADBOT_CALIBRATION_REFERENCE_DISTANCE_FEET=20.16
# DADBOT_CALIBRATION_REFERENCE_PIXEL_START_X=0
# DADBOT_CALIBRATION_REFERENCE_PIXEL_END_X=269

# Optional: Override detection settings
# DADBOT_DETECTION_MODEL_ID=rf-detr-base
# DADBOT_DETECTION_CONFIDENCE_THRESHOLD=0.3

# Optional: Override tracking settings
# DADBOT_TRACKING_SPEED_LIMIT_MPH=25.0

# Optional: Override reporting settings
# DADBOT_REPORTING_AGGREGATION_WINDOW_SECONDS=300
